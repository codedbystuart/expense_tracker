{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["import"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 1,
        "Quotes": 1,
        "no-unused-vars": 0
    }
}
